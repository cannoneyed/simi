{
  "presets": [
    "es2015",
    "react",
    "stage-1"
  ],
  "sourceRoot": "./src",
  "plugins": [
    "transform-react-jsx-source",
    "transform-decorators-legacy",
    "transform-class-properties",
  ],
  "env": {
    "development": {
      "plugins": [
        [
          "styled-components", {
            "displayName": true,
            "minify": true
          }
        ]
      ]
    },
    "production": {
      "plugins": [
        [
          "styled-components", {
            "displayName": false,
            "minify": true
          }
        ]
      ]
    }
  }
}
