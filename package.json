    "react-addons-test-utils": "~15.0.2",
    "react-transform-hmr": "~1.0.4",
    "resolve-url-loader": "^1.6.0",
    "style-loader": "~0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "~1.13.0",
    "webpack-cordova-plugin": "^0.1.6",
    "webpack-dev-server": "~1.14.1"
  },
  "dependencies": {
    "bluebird": "^3.3.5",
    "dotenv": "^2.0.0",
    "jquery": "^3.2.1",
