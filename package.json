{
  "name": "subghost",
  "version": "0.0.1",
  "description": "web audio comic demo",
  "author": {
    "name": "Andy Coenen",
    "email": "andrew.coenen@gmail.com"
  },
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --display-chunks --display-modules --progress --json > stats.json",
    "clean": "del-cli ./docs",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --inline --progress",
    "lint": "eslint -c .eslintrc.yml *.js ./src ./server",
    "prebuild": "npm-run-all clean lint",
    "start": "npm run dev",
    "test": "cross-env NODE_ENV=test karma start --single-run",
    "test:watch": "cross-env NODE_ENV=test karma start",
    "test:generate": "babel-node tests/generate-tests"
  },
  "devDependencies": {
    "babel-core": "6.17.0",
